<?xml version='1.0' encoding='utf-8'?>
<widget id="com.MexicoWeb16.speed10" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Speed 10</name>
    <description>
        A game in which users continuously select two numbers that add up to ten in a limited amount of time to get the highest score possible.
    </description>
    <author email="mexicoweb16@gmail.com" href="http://cordova.io">
        MexicoWeb16
    </author>
    <content src="index.html" />
	<icon src = "icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
